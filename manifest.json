{
    "manifest_version": 3,
    "name": "Quick Search Enhancer",
    "version": "1.0",
    "description": "Quickly search selected text on multiple search engines.",
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon.png",
            "48": "icons/icon.png",
            "128": "icons/icon.png"
        }
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
  